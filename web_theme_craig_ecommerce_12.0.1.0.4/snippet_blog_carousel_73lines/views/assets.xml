<odoo>

    <!-- Snippets & Thumbnails -->
    <template id="s_blog_carousel_place_in_tab" inherit_id="website.snippets">
        <xpath
                expr="//div[@id='snippet_feature']/div[hasclass('o_panel_body')]" position="inside">
            <t t-snippet="snippet_blog_carousel_73lines.s_blog_carousel"
               t-thumbnail="/snippet_blog_carousel_73lines/static/src/img/blog-carousel.png" />
        </xpath>
    </template>

</odoo>
