<odoo>
    <!-- ======== Product Tab Carousel ======== -->
    <template id="product_tab_carousel" name="Product Tab Carousel">
        <section class="mt32 mb32">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <ul class="nav nav-tabs" role="tablist" id="proTab">
                            <li class="nav-item">
                                <a class="nav-link active" data-toggle="tab" href="#tab1" role="tab">Tab 1</a>
                            </li>
                            <li>
                                <a class="nav-link" data-toggle="tab" href="#tab2" role="tab">Tab 2</a>
                            </li>
                            <li>
                                <a class="nav-link" data-toggle="tab" href="#tab3" role="tab">Tab 3</a>
                            </li>
                        </ul>
                        <div class="tab-content" id="myTabContent">
                            <div class="tab-pane fade show active" id="tab1" role="tabpanel">
                                <div class="oe_structure"/>
                            </div>
                            <div class="tab-pane fade in" id="tab2" role="tabpanel">
                                <div class="oe_structure"/>
                            </div>
                            <div class="tab-pane fade in" id="tab3" role="tabpanel">
                                <div class="oe_structure"/>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </template>
</odoo>
