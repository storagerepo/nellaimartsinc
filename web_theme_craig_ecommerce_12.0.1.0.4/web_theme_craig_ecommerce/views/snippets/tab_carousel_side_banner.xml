<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template id="tab_carousel_banner_side_banner"
              name="Tab Carousel And Side Banner">
        <section>
            <div class="container-fluid">
                <div class="row">
                    <div id="left_side_block"
                         class="col-lg-3 col-md-12 order-1">
                        <section class="s_one_promo_banner">
                            <div class="container bg-white">
                                <div class="row">
                                    <div class="col-md-12 col-sm-12 col-xs-12 mt16 mb16">
                                        <div class="oe_structure">
                                            <div class="ttbannerblock layout2 style1">
                                                <div class="ttbanner1 ttbanner">
                                                    <div id="ttbanner-img"
                                                         class="ttbanner-img">
                                                        <a>
                                                            <img src="/web_theme_craig_ecommerce/static/src/img/left-banner-1-231x460.jpg"
                                                                 class="mx-auto img img-fluid"/>
                                                            <span class="hover1 hover"/>
                                                            <span class="hover2 hover"/>
                                                            <span class="hover3 hover"/>
                                                            <span class="hover4 hover"/>
                                                        </a>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </section>

                        <section class="s_service">
                            <div class="container bg_white">
                                <div class="row">
                                    <div class="col-md-12 col-sm-12 col-xs-12">
                                        <div class="oe_structure">
                                            <div id="ttcmsleftservices">
                                                <h2 class="title_block o_default_snippet_text">
                                                    Our Servies
                                                </h2>
                                                <div class="block_content craig_side">
                                                    <div class="ttshipping">
                                                        <div class="ttcontent_inner col-md-12">
                                                            <div class="service">
                                                                <div class="ttshipping_img service-icon"/>
                                                                <div class="service-content">
                                                                    <div class="service-title o_default_snippet_text">
                                                                        Free
                                                                        Shipping
                                                                    </div>
                                                                    <div class="o_default_snippet_text">
                                                                        Deliver
                                                                        to Door
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="ttsupport">
                                                        <div class="ttcontent_inner col-md-12">
                                                            <div class="service">
                                                                <div class="ttsupport_img service-icon"/>
                                                                <div class="service-content">
                                                                    <div class="service-title o_default_snippet_text">
                                                                        24x7
                                                                        Support
                                                                    </div>
                                                                    <div class="o_default_snippet_text">
                                                                        in safe
                                                                        hands
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="ttmoneyback">
                                                        <div class="ttcontent_inner col-md-12">
                                                            <div class="service">
                                                                <div class="ttmoneyback_img service-icon"/>
