                                                        data-object_name="product.template"
                                                        data-objects_limit="10"
                                                        data-objects_in_slide="4"
                                                        data-objects_in_row="2"/>
                                            </div>
                                            <div class="tab-pane container fade"
                                                 id="menu4">
                                                <section
                                                        class="s_product_carousel mb8 js_get_objects product_carousel headline_hidden"
