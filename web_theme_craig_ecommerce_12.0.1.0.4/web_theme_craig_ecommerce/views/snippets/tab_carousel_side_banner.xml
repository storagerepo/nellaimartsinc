                            <div class="container">
                                <div class="row">
                                    <div class="col-md-12 col-sm-12 col-xs-12 mt16 mb16">
                                        <div class="oe_structure">
                                            <div class="ttbannerblock layout2 style1">
                                                <div class="ttbanner1 ttbanner">
                                                    <div id="ttbanner-img"
