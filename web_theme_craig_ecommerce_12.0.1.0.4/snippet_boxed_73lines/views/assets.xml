        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/snippet_boxed_73lines/static/src/js/s_state_box.js"/>
            <script type="text/javascript" src="/snippet_boxed_73lines/static/src/js/s_client_intro_editor.js"/>
            <script src="/snippet_boxed_73lines/static/src/js/snippet.ext.editor.js" type="text/javascript"/>
            <script type="text/javascript" src="/snippet_boxed_73lines/static/src/js/bar_chart.js"/>
            <script src="/snippet_boxed_73lines/static/src/js/rte_options.js" type="text/javascript"/>
            <script type="text/javascript" src="/snippet_boxed_73lines/static/src/js/progress_bar.js"/>
            <script src="/snippet_boxed_73lines/static/src/js/s_media_block_editor.js" type="text/javascript"/>
        </xpath>
