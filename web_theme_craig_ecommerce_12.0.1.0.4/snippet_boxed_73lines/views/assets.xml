            <link href="/snippet_boxed_73lines/static/src/scss/s_promo_images.scss"
                  rel="stylesheet" type="text/scss"/>
            <link href="/snippet_boxed_73lines/static/src/scss/s_three_boxes_with_icon.scss"
                  rel="stylesheet" type="text/scss"/>
            <link href="/snippet_boxed_73lines/static/src/scss/s_ecommerce_terms.scss"
                  rel="stylesheet" type="text/scss"/>
            <link href="/snippet_boxed_73lines/static/src/scss/s_progress_bar.scss"
                  rel="stylesheet" type="text/scss"/>
            <link href="/snippet_boxed_73lines/static/src/scss/odometer-theme-minimal.scss"
