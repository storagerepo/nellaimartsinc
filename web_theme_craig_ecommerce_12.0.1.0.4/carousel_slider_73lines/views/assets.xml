<odoo>
    <!-- Scss & Css & JS Assets -->
    <template id="assets_frontend_js" name="Latest Objects JS frontend assets" inherit_id="website.assets_frontend">
        <xpath expr="//link[last()]" position="after">
            <link href="/carousel_slider_73lines/static/src/less-css/owl.carousel.css"
                  rel="stylesheet" type="text/css" />
            <link href="/carousel_slider_73lines/static/src/less-css/custom_carousel.scss"
                  rel="stylesheet" type="text/scss"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/carousel_slider_73lines/static/src/js/owl.carousel.js"/>
            <script type="text/javascript" src="/carousel_slider_73lines/static/src/js/carousel_frontend.js"/>
        </xpath>
    </template>

    <!-- scss & Css & JS Assets -->
    <template id="assets_editor" name="Latest Objects JS editor assets" inherit_id="website.assets_editor">
        <xpath expr="//script[last()]" position="after">
            <script src="/carousel_slider_73lines/static/src/js/carousel_editor.js" type="text/javascript"/>
        </xpath>
    </template>
</odoo>
